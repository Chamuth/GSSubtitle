<UserControl x:Class="GSSubtitle.Windows.Special_Charcters.SpecialCharButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GSSubtitle.Windows.Special_Charcters"
             mc:Ignorable="d" Height="50.167" FontSize="13.333" Width="50"
             x:Name="root" Margin="2">
    <Border x:Name="Border" BorderThickness="1" CornerRadius="1" BorderBrush="{Binding Borderbrush ,ElementName=root}" Margin="0">
        <TextBlock x:Name="Character" Text="S" FontSize="20" TextAlignment="Center" Margin="0,0,0,0" Background="#7F0ED9EE" />
    </Border>
</UserControl>
